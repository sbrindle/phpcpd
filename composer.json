{
    "name": "sbrindle/phpcpd",
    "description": "Copy/Paste Detector (CPD) for PHP code.",
    "homepage": "https://github.com/sbrindle/phpcpd",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sebastian Bergmann",
            "email": "sebastian@phpunit.de",
            "role": "lead"
        },
        {
            "name": "SÃ©bastien Brindle",
            "role": "Fork maintainer"
        }
    ],
    "support": {
        "issues": "https://github.com/sbrindle/phpcpd/issues"
    },
    "config": {
        "platform": {
            "php": "8.1.0"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-dom": "*",
        "sebastian/cli-parser": ">=2.0",
        "sebastian/version": ">=4.0",
        "phpunit/php-file-iterator": ">=4.0",
        "phpunit/php-timer": ">=6.0",
        "ext-mbstring": "*"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "bin": [
        "phpcpd"
    ],
    "extra": {
        "branch-alias": {
            "dev-main": "7.0-dev"
        }
    }
}
